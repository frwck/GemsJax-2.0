<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='gemsjax'>
	<!-- Inherit the core Web Toolkit stuff.                        -->
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.http.HTTP" />
	<inherits name="com.google.gwt.xml.XML" />

	<!-- Inherit the default GWT style sheet.  You can change       -->
	<!-- the theme of your GWT application by uncommenting          -->
	<!-- any one of the following lines.                            -->
	<!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/>  -->
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

	<!-- Smart GWT                                      -->
	<inherits name="com.smartgwt.SmartGwt" />

	<inherits name="com.google.gwt.i18n.I18N" />
	<inherits name='com.google.gwt.xml.XML' />
	<inherits name="com.google.gwt.http.HTTP" />

	<inherits name="com.google.common.collect.Collect" />

	<!-- Development tool -->
	<inherits name="com.smartgwt.tools.SmartGwtTools" />


	<inherits name="gwtupload.GWTUpload" />
	<!-- Load dinamically predefined styles in the library when the application starts -->
	<stylesheet src="Upload.css" />

	<!-- Smart GWT Design -->
	<!--  <inherits name="com.smartclient.theme.silverwave.SilverWave"/> -->

	<!-- Specify the app entry point class.                         -->

	<!-- Specify the paths for translatable code                    -->
	<source path='client' />
	<source path='shared' />
	<entry-point class="org.gemsjax.client.admin.GemsJaxClient" />

	<!-- compile only for chrome/safari  -->
	<set-property name="user.agent" value="safari" />

	<!-- Development mode -->
	<set-property name="compiler.emulatedStack" value="true" />
	<set-configuration-property
		name="compiler.emulatedStack.recordFileNames" value="true" />
	<set-configuration-property
		name="compiler.emulatedStack.recordLineNumbers" value="true" />
	<!--  <inherits name="com.google.gwt.junit.JUnit"/> -->



	<!-- languages -->
	<extend-property name="locale" values="de" />
</module>
