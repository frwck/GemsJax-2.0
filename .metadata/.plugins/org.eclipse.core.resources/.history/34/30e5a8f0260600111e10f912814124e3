package org.gemsjax.shared.tag;

import java.util.List;


/**
 * Adds the ability to hold tags. Tags are useful for searching.
 * @author Hannes Dorfmann
 *
 */
public interface Tagable {

	public void addTag(Tag t) throws TagException;
	public void removeTag(Tag t);
	
	public List<Tag> getTags();
	
}
