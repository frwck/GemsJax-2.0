package org.gemsjax.server.persistence.user;

import org.gemsjax.shared.user.RegisteredUser;
import org.gemsjax.shared.user.UserOnlineState;



public class RegisteredUserImpl implements RegisteredUser {
	
	private int id;
	private String displayedName;
	private String email;
	private String username;
	private 
	UserOnlineState onlineState ;
	
	public RegisteredUserImpl()
	{
		onlineState = UserOnlineState.OFFLINE;
	}
	
	

	@Override
	public String getEmail() {
		return email;
	}

	@Override
	public String getUsername() {
		return username;
	}

	@Override
	public String getDisplayedName() {
		return displayedName;
	}

	@Override
	public int getId() {
		return id;
	}

	@Override
	public UserOnlineState getOnlineState() {
		return onlineState;
	}

}
